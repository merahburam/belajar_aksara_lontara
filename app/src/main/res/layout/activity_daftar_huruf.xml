<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".belajaraksara.DaftarHuruf">

    <ImageView
        android:id="@+id/background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@null"/>

    <ImageView
        android:id="@+id/panelJudul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#0000"
        android:scaleType="fitXY"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:contentDescription="@null"/>

    <ImageView
        android:id="@+id/Judul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#0000"
        android:scaleType="fitXY"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="17dp"
        android:contentDescription="@null"/>

    <ImageButton
        android:id="@+id/tombolkembali"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#0000"
        android:layout_marginTop="293dp"
        android:scaleType="fitXY"
        android:contentDescription="@null"/>

    <ImageButton
        android:id="@+id/tombolaksaradasar"
        android:layout_width="@dimen/tombolBermain"
        android:layout_height="@dimen/tombolBermain"
        android:background="#0000"
        android:scaleType="fitXY"
        android:layout_centerVertical="true"
        android:layout_marginStart="140dp"
        android:contentDescription="@null"/>

    <ImageButton
        android:id="@+id/tombolaksarabunyi"
        android:layout_width="@dimen/tombolBermain"
        android:layout_height="@dimen/tombolBermain"
        android:background="#0000"
        android:scaleType="fitXY"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:contentDescription="@null"/>


    <ImageButton
        android:id="@+id/tombolaksarasuara"
        android:layout_width="@dimen/tombolBermain"
        android:layout_height="@dimen/tombolBermain"
        android:background="#0000"
        android:scaleType="fitXY"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="140dp"
        android:contentDescription="@null"/>

</RelativeLayout>
